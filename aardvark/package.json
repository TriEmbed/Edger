{
  "index": "Aardvark",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "description": "An out of box boilerplate based on vuetify with multiple preset enhancement",
  "keywords": [
    "vuetify",
    "material-design",
    "MD",
    "vue",
    "@vue/composition-api",
    "TencentMap",
    "TMap",
    "tencent-map"
  ],
  "engines": {
    "node": ">=14.18.1"
  },
  "scripts": {
    "serve": "vue-cli-service serve --mode development",
    "build": "vue-cli-service build --mode production",
    "build:report": "vue-cli-service build --report --mode production && cd dist && live-server --open=./report.html",
    "lint": "vue-cli-service lint",
    "lint:style": "stylelint **/*.{html,vue} --custom-syntax postcss-html",
    "lint:css": "stylelint **/*.{css,sass,scss} --syntax scss",
    "test:unit": "vue-cli-service test:unit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test:unit"
    }
  },
  "dependencies": {
    "@mdi/js": "7.0.96",
    "@vue/composition-api": "^1.4.0",
    "async-mutex": "^0.4.0",
    "await": "^0.2.6",
    "axios": "^0.24.0",
    "bootstrap-reboot-import": "^4.1.3",
    "bootstrap-reboot.min.css": "link:bootstrap/dist/css/bootstrap-reboot.min.css",
    "core-js": "^3.25.0",
    "cors": "^2.8.5",
    "lodash-es": "^4.17.21",
    "material-design-icons-iconfont": "^6.1.1",
    "menuable": "link:vuetify/src/mixins/menuable",
    "nprogress": "^0.2.0",
    "qs": "^6.11.0",
    "serialport": "^10.5.0",
    "store": "link:@/store",
    "vue": "^2.6.14",
    "vue-axios-cors": "^1.0.1",
    "vue-cookies": "^1.8.1",
    "vue-img-inputer": "^2.1.6",
    "vue-router": "^3.5.3",
    "vuetify": "^2.5.10",
    "vuex": "^3.6.2",
    "vuex-persist": "^3.1.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.5",
    "@vue/cli-plugin-babel": "^4.5.15",
    "@vue/cli-plugin-eslint": "^4.5.15",
    "@vue/cli-plugin-unit-jest": "^4.5.15",
    "@vue/cli-service": "^4.5.15",
    "@vue/test-utils": "^1.3.0",
    "axios-mock-adapter": "^1.20.0",
    "babel-eslint": "^7.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-preset-env": "^1.7.0",
    "compression-webpack-plugin": "^6.1.1",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^7.20.0",
    "husky": "^4.0.0",
    "@serialport/list": "^10.5.0",
    "lint-staged": "^12.1.2",
    "live-server": "^1.2.1",
    "lodash-webpack-plugin": "^0.11.6",
    "sass": "^1.43.4",
    "sass-loader": "^10.2.0",
    "style-resources-loader": "^1.4.1",
    "stylelint": "^13.13.1",
    "stylelint-codeframe-formatter": "^1.0.5",
    "stylelint-config-sass-guidelines": "^8.0.0",
    "stylelint-scss": "^3.20.1",
    "stylelint-webpack-plugin": "^2.3.1",
    "terser-webpack-plugin": "^2.2.1",
    "vue-template-compiler": "^2.6.14"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
